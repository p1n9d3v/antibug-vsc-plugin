<!DOCTYPE html>
<html lang="en">
  <head>
    <%-include('head.ejs') %>
    <title>Contract and Interaction</title>
  </head>
  <body>
    <div class="functions">
      <div class="function">
        <div class="function__interaction">
          <button>owner</button>
          <input />
          <div class="function__show-arguments">
            <i class="fa fa-chevron-down"></i>
          </div>
        </div>
        <div class="function__arguments hidden">
          <div class="function__argument">
            <div>type name</div>
            <input />
          </div>
          <div class="function__argument">
            <div>type namesdfasdfasdf</div>
            <input />
          </div>
        </div>
      </div>

      <div class="function">
        <div class="function__interaction">
          <button class="pure">owner</button>
          <input />
          <div class="function__show-arguments">
            <i class="fa fa-chevron-down"></i>
          </div>
        </div>
      </div>

      <div class="function">
        <div class="function__interaction">
          <button class="payable">owner</button>
          <input />
          <div class="function__show-arguments">
            <i class="fa fa-chevron-down"></i>
          </div>
        </div>
        <div class="function__arguments hidden">
          <div class="function__argument">
            <div>type name</div>
            <input />
          </div>
          <div class="function__argument">
            <div>type namesdfasdfasdf</div>
            <input />
          </div>
        </div>
      </div>
    </div>
    <!-- SCRIPTS -->
    <%-include('scripts.ejs') %>
  </body>
</html>
